{
  "name": "jovetrajkoski/php-user-api",
  "description": "A simple PHP Core REST API for managing users",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "start": "php -S localhost:8000 -t public"
  },
  "config": {
    "optimize-autoloader": true
  }
}
